{
  "kind": "collectionType",
  "collectionName": "tournaments",
  "info": {
    "singularName": "tournament",
    "pluralName": "tournaments",
    "displayName": "Tournament",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "string"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "tournaments"
    },
    "scores": {
      "type": "json",
      "required": true
    },
    "sessions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::session.session",
      "mappedBy": "tournament"
    },
    "apiid": {
      "type": "uid"
    },
    "synced": {
      "type": "boolean",
      "default": true
    },
    "firstsync": {
      "type": "boolean",
      "default": true
    },
    "finished": {
      "type": "boolean",
      "default": false
    },
    "description": {
      "type": "text"
    },
    "score_system": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::score-system.score-system"
    },
    "when": {
      "type": "date"
    },
    "archer_choices": {
      "type": "json"
    }
  }
}
